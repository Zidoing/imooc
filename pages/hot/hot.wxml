<view class="tabs">
    <view
            wx:for="{{rankTypes}}" wx:key="type"
            class="tab {{rankType===item.type ? 'active' : ''}}"
            bind:tap="handleTabChange"
            data-type="{{item.type}}"
    >{{item.title}}
        <view wx:if="{{rankType===item.type ? 'active' : ''}}" class="active-bar"></view>
    </view>

</view>

<view class="time">
    <view wx:for="{{rankPeriods}}"
          data-type="{{item.value}}"
          bind:tap="handlePeriodChange"
          class="period {{rankPeriod === item.value ? 'period-active' : ''}}">
        {{item.title}}
    </view>
</view>

<view class="list">
    <view class="item">
        <image class="item-image" src="https://img1.mukewang.com/szimg/5f1eafb709e18bf912000676.png"></image>
        <view class="item-title">
            <view class="item-title-inner">
                ios页面传值
            </view>
        </view>
        <view class="item-number">
            <image class="item-number-img" src="../../resources/first.png"></image>
        </view>
    </view>
</view>


